{
    "bleurgh": {
        "base": {
            "background-color": "rgba(0,0,0,0.3)",
            "fill-color": "rgba(220,80,50,1)",
            "border-width": 1,
            "padding": 5,
            "border-color": "rgba(0,0,0,0.4)",
            "margin": 5,
            "text-color": "rgba(255,255,255,0.8)",
            "text-padding": 5,
            "font-family": "assets/fonts/NewMediaFett.ttf",
            "font-size": 10,
            "align-content": "center",
            "justify-content": "space-around"
        },
        
        "panel": {
            "width": 300,
            "direction": "vertical",
            "show-header": true,
            "background-color": "rgba(0,0,0,0.3)"
        },
        
        "group": {
            "width": "100%",
            "background-color": "rgba(0,0,0,0.2)",
            "padding": 0,
            "margin": 0,
            "margin-bottom": 1
        },
        
        "group-header": {
            "height": 39,
            "margin": 0,
            "width": 322,
            "background-color": "rgba(0,0,0,0.2)",
            "text-color": "rgba(250,250,250,1)"
        },
        
        "slider": {
          "fill-color": "rgba(220,80,50,1)",
          "precision": 3,
            "padding": 10,
          "width": "100%"
        },
            
        "toggle": {
          "height": 40,
          "background-color": "rgba(220,80,50,0.1)",
          "border-color": "rgba(220,80,50,0.9)",
          "fill-color": "rgba(220,80,50,0.9)",
          "type": "radio",
          "border-width": 2
        },
            
        "button": {
          "fill-color": "rgba(220,80,50,1)",
          "background-color": "rgba(220,80,50,0.3)",
          "type": "fullsize"
        },
            
        "label": {
          "text-color": "rgba(220,80,50,1)",
          "border-color": "rgba(0,0,0,0)"
        },
            
        "graphics": {
          "background-color": "rgba(0,0,0,0.4)"
        },
      
        "value-plotter": {
          "fill-color": "rgba(220,80,50,1)",
          "height": 40,
          "precision": 3,
          "width": "100%"
        },
            
        "function-plotter": {
          "fill-color": "rgba(220,80,50,1)",
          "precision": 3
        },
            
        "tabs": {
          "padding": 20
        }
      
      
    }
}